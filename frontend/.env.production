# Production environment variables for NANNOTES Frontend
# Railway will automatically replace RAILWAY_STATIC_URL with your backend URL

# API Base URL - Railway automatically injects the backend URL
REACT_APP_API_URL=https://nannotes-backend-production.up.railway.app/api

# Build optimizations
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# App Configuration
REACT_APP_ENV=production
REACT_APP_NAME=NANNOTES
REACT_APP_VERSION=1.0.0

# Performance optimizations
INLINE_RUNTIME_CHUNK=false
